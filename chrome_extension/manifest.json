{
    "manifest_version": 2,
    "name": "restbot",
    "version": "1.0",
    "background": {
        "scripts": [
            "jquery-2.1.4.min.js",
            "socket.io.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "default.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ]
}